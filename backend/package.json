{
    "name": "@das-kitchen/backend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
