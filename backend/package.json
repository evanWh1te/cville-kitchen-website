        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^6.8.1",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.10",
        "prisma": "^6.19.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie-parser": "^1.4.10",
        "@types/cors": "^2.8.13",
